import matplotlib.pyplot as plt
import numpy as np

def y(n):
    return ((n+1)*(n+2)*(n+3)) / 3
n_values = np.arange(0, 10, 1)
y_values = [y(n) for n in n_values]
plt.scatter(n_values, y_values, marker='o', color='b')
plt.vlines(n_values, 0, y_values, colors='gray', linestyles='dashed')
plt.xlabel('n')
plt.ylabel('y(n)')
plt.grid(True)
plt.show()
