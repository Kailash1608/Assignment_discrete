#include <stdio.h>
#include<math.h>

int main()
{
    int n;
    printf("The series is given by 1*2+2*3+3*4.....\n");
    printf("Enter the number of terms till which you want the sum of the series:");
    scanf("%d",&n);
    printf("The sum is %d",(int)(pow(n,3)+3*pow(n,2)+2*n)/3);
    

    return 0;
}
